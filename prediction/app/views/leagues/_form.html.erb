<%= form_for(@league)  do |f| %>
  
    <%= f.label :league_name %><br>
    <%= f.text_field :league_name %>
  <br>
    <%= f.label :start_date %><br>
    <%= f.date_select :start_date %>
  <br>
    <%= f.label :end_date %><br>
    <%= f.date_select :end_date %>
  <br>
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  <br>
    <%= f.label :status %><br>
    <%= f.text_field :status %>
  <br>
    <%= f.submit %>
<% end %> 

