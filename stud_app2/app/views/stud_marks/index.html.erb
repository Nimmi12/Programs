<h1>Listing stud_marks</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Subject</th>
      <th>Mark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stud_marks.each do |stud_mark| %>
      <tr>
        <td><%= stud_mark.student_id%></td>
        <td><%= stud_mark.subject_id%></td>
        <td><%= stud_mark.mark %></td>
        <td><%= link_to 'Show', stud_mark %></td>
        <td><%= link_to 'Edit', edit_stud_mark_path(stud_mark) %></td>
        <td><%= link_to 'Destroy', stud_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stud mark', new_stud_mark_path %>
